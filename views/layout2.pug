    script(src="./javascripts/myscript2.js")
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    .menu_bar
      button.save.btn.btn-primary.btn-sm Save
      button.fork.btn.btn-primary.btn-sm Fork
      //-var o = document.location.origin;
      -var h = '/out/' + doc._id;
      a.btn.btn-primary.btn-sm(href=h target='_blank') Output in new tab
      button.expand_html.btn.btn-info.btn-sm Expand HTML
      button.expand_css.btn.btn-info.btn-sm Expand CSS
      button.expand_js.btn.btn-info.btn-sm Expand Js
      button.reset_view.btn.btn-info.btn-sm Reset view
      a(href='https://github.com/arunkumar413/Code-Ground' target="_blank" )
        i.fab.fa-github.fa-3x
  block content
